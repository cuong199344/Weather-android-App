<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/custom_background_fragment"
    tools:context=".FragmentPage.MapFragment">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/textViewUserName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/hello"
            android:textSize="@dimen/textview_tagname_top"
            android:drawableStart="@drawable/ic_singleuser"
            android:layout_marginTop="5dp"
            android:textColor="@color/white"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:drawablePadding="5dp"/>
        <TextView
            android:id="@+id/textViewUserNameValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/textview_tagname_top"
            android:layout_marginTop="5dp"
            android:textColor="@color/white"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/textViewUserName"/>
        <View
            android:id="@+id/viewTop"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/custom_background_fragment_dashboard_top"
            app:layout_constraintHeight_percent="0.26"
            app:layout_constraintWidth_percent="0.9"
            app:layout_constraintTop_toBottomOf="@id/textViewUserName"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:elevation="@dimen/elevation_dashboard_view"/>
        <View
            android:id="@+id/viewMid"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/custom_background_fragment_dashboard_mid"
            app:layout_constraintHeight_percent="0.64"
            app:layout_constraintWidth_percent="0.9"
            app:layout_constraintTop_toBottomOf="@id/viewTop"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:elevation="@dimen/elevation_dashboard_view"/>

        <View
            android:id="@+id/viewRainfall"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/border_view_value"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            app:layout_constraintHeight_percent="0.14"
            app:layout_constraintStart_toStartOf="@id/viewMid"
            app:layout_constraintEnd_toEndOf="@id/viewMid"
            app:layout_constraintTop_toTopOf="@id/viewMid"
            android:elevation="@dimen/elevation_dashboard_view"/>
        <View
            android:id="@+id/viewRainfallImg"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.11"
            app:layout_constraintWidth_percent="0.22"
            android:background="@drawable/custom_img_background"
            android:elevation="15dp"
            app:layout_constraintStart_toStartOf="@id/viewRainfall"
            app:layout_constraintTop_toTopOf="@id/viewRainfall"
            app:layout_constraintBottom_toBottomOf="@id/viewRainfall"
            android:layout_marginLeft="30dp"
            />


        <View
            android:id="@+id/viewHumidity"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/border_view_value"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            app:layout_constraintHeight_percent="0.14"
            app:layout_constraintStart_toStartOf="@id/viewMid"
            app:layout_constraintEnd_toEndOf="@id/viewMid"
            app:layout_constraintTop_toBottomOf="@id/viewRainfall"
            android:elevation="@dimen/elevation_dashboard_view"/>
        <View
            android:id="@+id/viewHumidityImg"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.11"
            app:layout_constraintWidth_percent="0.22"
            android:background="@drawable/custom_img_background"
            android:elevation="15dp"
            app:layout_constraintStart_toStartOf="@id/viewHumidity"
            app:layout_constraintTop_toTopOf="@id/viewHumidity"
            app:layout_constraintBottom_toBottomOf="@id/viewHumidity"
            android:layout_marginLeft="30dp"
            />


        <View
            android:id="@+id/viewWindDirection"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/border_view_value"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            app:layout_constraintHeight_percent="0.14"
            app:layout_constraintStart_toStartOf="@id/viewMid"
            app:layout_constraintEnd_toEndOf="@id/viewMid"
            app:layout_constraintTop_toBottomOf="@id/viewHumidity"
            android:elevation="@dimen/elevation_dashboard_view"/>
        <View
            android:id="@+id/viewWindDirectionImg"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.11"
            app:layout_constraintWidth_percent="0.22"
            android:background="@drawable/custom_img_background"
            android:elevation="15dp"
            android:layout_marginLeft="30dp"
            app:layout_constraintStart_toStartOf="@id/viewWindDirection"
            app:layout_constraintTop_toTopOf="@id/viewWindDirection"
            app:layout_constraintBottom_toBottomOf="@id/viewWindDirection"
            />


        <View
            android:id="@+id/viewWindspeed"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/border_view_value"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            app:layout_constraintHeight_percent="0.14"
            app:layout_constraintStart_toStartOf="@id/viewMid"
            app:layout_constraintEnd_toEndOf="@id/viewMid"
            app:layout_constraintTop_toBottomOf="@id/viewWindDirection"
            android:elevation="@dimen/elevation_dashboard_view"/>
        <View
            android:id="@+id/viewWindspeedImg"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.11"
            app:layout_constraintWidth_percent="0.22"
            android:background="@drawable/custom_img_background"
            android:elevation="15dp"
            android:layout_marginLeft="30dp"
            app:layout_constraintStart_toStartOf="@id/viewWindspeed"
            app:layout_constraintTop_toTopOf="@id/viewWindspeed"
            app:layout_constraintBottom_toBottomOf="@id/viewWindspeed"

            />


        <TextView
            android:id="@+id/textViewWindSpeed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/orange"
            android:text="@string/wind_speed"
            app:layout_constraintTop_toTopOf="@id/viewWindspeed"
            app:layout_constraintBottom_toTopOf="@id/WindSpeedValue"
            app:layout_constraintStart_toStartOf="@id/WindSpeedValue"
            app:layout_constraintEnd_toEndOf="@id/WindSpeedValue"
            android:elevation="10dp"
            />
        <TextView
            android:id="@+id/WindSpeedValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            app:layout_constraintStart_toEndOf="@id/WindSpeedImage"
            app:layout_constraintEnd_toEndOf="@id/viewWindspeed"
            app:layout_constraintTop_toTopOf="@id/viewWindspeed"
            app:layout_constraintBottom_toBottomOf="@id/viewWindspeed"
            android:text="@string/textview"
            android:textColor="#FFFFFF"
            android:textSize="30dp"
            android:textStyle="bold"
            android:elevation="@dimen/elevation_dashboard_view"
            />


        <TextView
            android:id="@+id/PlaceValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            app:layout_constraintStart_toStartOf="@id/TemperatureValue"
            app:layout_constraintEnd_toEndOf="@id/TemperatureValue"
            app:layout_constraintTop_toBottomOf="@id/Date"
            app:layout_constraintBottom_toBottomOf="@id/viewTop"
            android:text="@string/textview"
            android:textColor="#FFFFFF"
            android:textSize="30dp"
            android:elevation="@dimen/elevation_dashboard_view"
            />

        <TextView
            android:id="@+id/textViewRainfall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/orange"
            android:text="@string/rainfall"
            app:layout_constraintTop_toTopOf="@id/viewRainfall"
            app:layout_constraintBottom_toTopOf="@id/RainfallValue"
            app:layout_constraintStart_toStartOf="@id/RainfallValue"
            app:layout_constraintEnd_toEndOf="@id/RainfallValue"
            android:elevation="10dp"
            />
        <TextView
            android:id="@+id/RainfallValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@id/viewRainfall"
            app:layout_constraintTop_toTopOf="@id/viewRainfall"
            app:layout_constraintBottom_toBottomOf="@id/viewRainfall"
            app:layout_constraintStart_toEndOf="@id/RainfallImage"
            android:text="@string/textview"
            android:textColor="#FFFFFF"
            android:textSize="30dp"
            android:textStyle="bold"
            android:elevation="10dp"
            android:layout_marginRight="10dp"
            />
        <TextView
            android:id="@+id/textViewHumidity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/orange"
            android:text="@string/humidity"
            app:layout_constraintTop_toTopOf="@id/viewHumidity"
            app:layout_constraintBottom_toTopOf="@id/HumidityValue"
            app:layout_constraintStart_toStartOf="@id/HumidityValue"
            app:layout_constraintEnd_toEndOf="@id/HumidityValue"
            android:elevation="10dp"
            />
        <TextView
            android:id="@+id/HumidityValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            app:layout_constraintStart_toEndOf="@id/HumidityImage"
            app:layout_constraintEnd_toEndOf="@id/viewHumidity"
            app:layout_constraintTop_toTopOf="@id/viewHumidity"
            app:layout_constraintBottom_toBottomOf="@id/viewHumidity"
            android:text="@string/textview"
            android:textColor="#FFFFFF"
            android:textSize="30dp"
            android:textStyle="bold"
            android:elevation="@dimen/elevation_dashboard_view"
            />
        <TextView
            android:id="@+id/textViewWinddirection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/orange"
            android:text="@string/wind_direction"
            app:layout_constraintTop_toTopOf="@id/viewWindDirection"
            app:layout_constraintBottom_toTopOf="@id/WindDirectionValue"
            app:layout_constraintStart_toStartOf="@id/WindDirectionValue"
            app:layout_constraintEnd_toEndOf="@id/WindDirectionValue"
            android:elevation="10dp"
            />
        <TextView
            android:id="@+id/WindDirectionValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:layout_marginRight="10dp"
            app:layout_constraintStart_toEndOf="@id/WindDirectionImage"
            app:layout_constraintEnd_toEndOf="@id/viewWindDirection"
            app:layout_constraintTop_toTopOf="@id/viewWindDirection"
            app:layout_constraintBottom_toBottomOf="@id/viewWindDirection"
            android:text="@string/textview"
            android:textColor="#FFFFFF"
            android:textSize="30dp"
            android:elevation="@dimen/elevation_dashboard_view"
            />

        <TextView
            android:id="@+id/TemperatureValue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@id/viewTop"
            app:layout_constraintEnd_toEndOf="@id/viewTop"
            app:layout_constraintTop_toTopOf="@id/viewTop"
            app:layout_constraintBottom_toBottomOf="@id/viewTop"
            android:layout_marginTop="40dp"
            android:layout_marginBottom="100dp"
            android:layout_marginStart="160dp"
            android:text="@string/textview"
            android:textColor="#FFFFFF"
            android:textSize="40dp"
            android:elevation="@dimen/elevation_dashboard_view"
            android:textStyle="bold"
            />
        <TextView
            android:id="@+id/Time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="20dp"
            android:textColor="#FFFFFF"
            app:layout_constraintTop_toTopOf="@id/viewTop"
            app:layout_constraintStart_toStartOf="@id/TemperatureValue"
            app:layout_constraintEnd_toEndOf="@id/TemperatureValue"
            app:layout_constraintBottom_toTopOf="@id/TemperatureValue"
            android:elevation="@dimen/elevation_dashboard_view"
            />
        <ImageView
            android:id="@+id/TypeWeather"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/cloudy_sunny"
            app:layout_constraintEnd_toStartOf="@id/TemperatureValue"
            app:layout_constraintStart_toStartOf="@id/viewTop"
            app:layout_constraintTop_toTopOf="@id/viewTop"
            app:layout_constraintBottom_toBottomOf="@id/viewTop"
            app:layout_constraintHeight_percent="0.15"
            app:layout_constraintWidth_percent="0.3"
            android:elevation="@dimen/elevation_dashboard_view"
            />
        <ImageView
            android:id="@+id/RainfallImage"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/rain"
            app:layout_constraintStart_toStartOf="@id/viewRainfallImg"
            app:layout_constraintTop_toTopOf="@id/viewRainfallImg"
            app:layout_constraintBottom_toBottomOf="@id/viewRainfallImg"
            app:layout_constraintEnd_toEndOf="@+id/viewRainfallImg"
            android:layout_marginTop="45dp"
            app:layout_constraintHeight_percent="0.09"
            app:layout_constraintWidth_percent="0.18"
            android:layout_marginBottom="50dp"
            android:elevation="20dp"
            />

        <ImageView
            android:id="@+id/HumidityImage"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/humidity"
            app:layout_constraintStart_toStartOf="@id/viewHumidityImg"
            app:layout_constraintTop_toTopOf="@id/viewHumidityImg"
            app:layout_constraintBottom_toBottomOf="@id/viewHumidityImg"
            app:layout_constraintEnd_toEndOf="@id/viewHumidityImg"
            app:layout_constraintHeight_percent="0.09"
            app:layout_constraintWidth_percent="0.18"
            android:elevation="20dp"
            />

        <ImageView
            android:id="@+id/WindDirectionImage"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/winddirection"
            app:layout_constraintStart_toStartOf="@id/viewWindDirectionImg"
            app:layout_constraintTop_toTopOf="@id/viewWindDirectionImg"
            app:layout_constraintBottom_toBottomOf="@id/viewWindDirectionImg"
            app:layout_constraintEnd_toEndOf="@+id/viewWindDirectionImg"
            app:layout_constraintHeight_percent="0.09"
            app:layout_constraintWidth_percent="0.18"
            android:elevation="20dp"
            />

        <ImageView
            android:id="@+id/WindSpeedImage"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/wind"
            app:layout_constraintStart_toStartOf="@id/viewWindspeedImg"
            app:layout_constraintTop_toTopOf="@id/viewWindspeedImg"
            app:layout_constraintBottom_toBottomOf="@id/viewWindspeedImg"
            app:layout_constraintEnd_toEndOf="@id/viewWindspeedImg"
            app:layout_constraintHeight_percent="0.09"
            app:layout_constraintWidth_percent="0.18"
            android:elevation="20dp"
            />
        <TextView
            android:id="@+id/Date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="20dp"
            android:textColor="#FFFFFF"
            app:layout_constraintStart_toStartOf="@id/TemperatureValue"
            app:layout_constraintEnd_toEndOf="@id/TemperatureValue"
            app:layout_constraintTop_toBottomOf="@id/TemperatureValue"
            app:layout_constraintBottom_toTopOf="@id/PlaceValue"
            android:elevation="@dimen/elevation_dashboard_view"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>